{
	"name": "@@name@@",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"typecheck": "tsc --noEmit",
		"lint": "npx eslint src/**/*.ts",
		"lint:fix": "npx eslint --fix src/**/*.ts",
		"dev": "",
		"build": "",
		"preview": "",
		"start": "",
		"deploy:production": "aerofoil deploy --local-package --production",
		"deploy:branch": "aerofoil deploy --local-package --branch"
	},
	"type": "module",
	"devDependencies": {
		"@aerofoil/aerofoil-core": "latest",
		"@aerofoil/af-ext-cloudflare": "latest",
		"@astrojs/check": "^0.2.0",
		"eslint": "latest",
		"eslint-config-aerofoil": "workspace:*",
		"tsconfig": "workspace:*",
		"type-fest": "latest",
		"typescript": "latest",
		"wrangler": "latest"
	},
	"dependencies": {
		"@astrojs/cloudflare": "^7.0.1",
		"@astrojs/prefetch": "^0.4.0",
		"@astrojs/sitemap": "^3.0.0",
		"astro": "^3.0.8",
		"zod": "v3.22.2"
	},
	"eslintConfig": {
		"ignorePatterns": [
			"dist"
		],
		"extends": [
			"aerofoil/eslint-page-astro"
		],
		"parserOptions": {
			"sourceType": "module",
			"project": "./tsconfig.json"
		},
		"settings": {
			"import/resolver": {
				"typescript": {
					"alwaysTryTypes": true,
					"project": "./tsconfig.json"
				}
			}
		}
	}
}