{
	"name": "@@name@@",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"typecheck": "tsc --noEmit",
		"lint": "npx eslint src/**/*.ts",
		"lint:fix": "npx eslint --fix src/**/*.ts",
		"dev": "aerofoil port-runner \"astro dev --port {port} --host\"",
		"build": "astro build",
		"preview": "aerofoil port-runner \"wrangler pages dev ./dist --port {port} --host\"",
		"start": "aerofoil no-op",
		"deploy:production": "aerofoil deploy --local-package --production",
		"deploy:branch": "aerofoil deploy --local-package --branch"
	},
	"type": "module",
	"devDependencies": {
		"@aerofoil/aerofoil-core": "latest",
		"@aerofoil/af-ext-cloudflare": "latest",
		"@astrojs/check": "^0.2.0",
		"eslint": "latest",
		"eslint-config-aerofoil": "workspace:*",
		"tsconfig": "workspace:*",
		"type-fest": "latest",
		"typescript": "latest",
		"wrangler": "latest"
	},
	"dependencies": {
		"@astrojs/cloudflare": "^7.0.2",
		"@astrojs/prefetch": "^0.4.0",
		"@astrojs/sitemap": "^3.0.0",
		"@sinclair/typebox": "latest",
		"astro": "^3.0.12"
	},
	"eslintConfig": {
		"ignorePatterns": [
			"dist"
		],
		"extends": [
			"aerofoil/eslint-page-astro"
		],
		"parserOptions": {
			"sourceType": "module",
			"project": "./tsconfig.json"
		},
		"settings": {
			"import/resolver": {
				"typescript": {
					"alwaysTryTypes": true,
					"project": "./tsconfig.json"
				}
			}
		}
	}
}